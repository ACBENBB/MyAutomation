    "/v1/token-transactions": {
      "get": {
        "operationId": "TokenTransactionsController_getTokenTransactions",
        "parameters": [
          {
            "name": "startDate",
            "required": false,
            "in": "query",
            "description": "Date greater than or equal to initialTimeStamp",
            "schema": {
              "format": "2023-12-09T13:30:38.867Z",
              "type": "string"
            }
          },
          {
            "name": "endDate",
            "required": false,
            "in": "query",
            "description": "Date lower than or equal to initialTimeStamp",
            "schema": {
              "format": "2023-12-09T13:30:38.867Z",
              "type": "string"
            }
          },
          {
            "name": "hasPierMatchId",
            "required": false,
            "in": "query",
            "description": "has pier match ID, applies only when type is transfer-ok",
            "schema": {
              "type": "boolean"
            }
          },
          {
            "name": "manuallyModified",
            "required": false,
            "in": "query",
            "description": "has been manually modified",
            "schema": {
              "type": "boolean"
            }
          },
          {
            "name": "manuallyCreated",
            "required": false,
            "in": "query",
            "description": "has been manually created",
            "schema": {
              "type": "boolean"
            }
          },
          {
            "name": "investorId",
            "required": false,
            "in": "query",
            "description": "senderUserId or receiverUserId",
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "issuerId",
            "required": false,
            "in": "query",
            "schema": {
              "type": "array",
              "items": {}
            }
          },
          {
            "name": "tokenId",
            "required": false,
            "in": "query",
            "schema": {
              "type": "array",
              "items": {}
            }
          },
          {
            "name": "type",
            "required": false,
            "in": "query",
            "schema": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [
                  "issuance",
                  "reallocation",
                  "transfer-ok",
                  "transfer-rejected",
                  "reclaim",
                  "destroy",
                  "freeze",
                  "unfreeze",
                  "redemption"
                ]
              }
            }
          },
          {
            "name": "processType",
            "required": false,
            "in": "query",
            "schema": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [
                  "empty",
                  "routine",
                  "amendment",
                  "non-routine"
                ]
              }
            }
          },
          {
            "name": "source",
            "required": false,
            "in": "query",
            "schema": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [
                  "other",
                  "import"
                ]
              }
            }
          },
          {
            "name": "q",
            "required": false,
            "in": "query",
            "description": "Search text from blockchain transaction id, transaction hash, investor id, investor wallet",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "orderDirection",
            "required": false,
            "in": "query",
            "schema": {
              "default": null,
              "enum": [
                "ASC",
                "DESC"
              ],
              "type": "string"
            }
          },
          {
            "name": "orderField",
            "required": false,
            "in": "query",
            "schema": {
              "default": null,
              "enum": [
                "createdAt",
                "updatedAt",
                "initialTimestamp"
              ],
              "type": "string"
            }
          },
          {
            "name": "includes",
            "required": false,
            "in": "query",
            "schema": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [
                  "blockchainTransaction",
                  "tokenIssuance"
                ]
              }
            }
          },
          {
            "name": "pierMatchId",
            "required": false,
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "transactionHash",
            "required": false,
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "offset",
            "required": false,
            "in": "query",
            "schema": {
              "minimum": 0,
              "default": 0,
              "type": "number"
            }
          },
          {
            "name": "limit",
            "required": false,
            "in": "query",
            "schema": {
              "minimum": 1,
              "default": 50,
              "type": "number"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Get Token Transactions By Filters",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetTokenTransactionsResponseDto"
                }
              }
            }
          }
        },
        "tags": [
          "Token Transactions"
        ]
      },
